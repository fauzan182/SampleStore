<html>
  <head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <title>Sample Store</title>
    <script src="https://api.sandbox.veritrans.co.id/v2/assets/js/veritrans.min.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <style>
      .container {
        width: 850px;
      }
      .btn {
        border: 0;
        padding: 8px 20px;
        color: white;
        border-radius: 0;
      }
      .btn:hover, .btn:focus, .btn:active:focus, .btn.active:focus {
        outline: 0 none;
        color: white;
      }
      .buy-button, .checkout-button {
        background-color: #FBB040;
      }
      .button-pay {
        background-color: #356635;
      }

      .buy-button:hover, .checkout-button:hover {
        background-color: #fa9b0e;
      }
      .btn+.btn {
        margin-left: 10px;
      }
      .visa {
        background: url(/assets/images/sprite.png) 0 -46px no-repeat;
      }
      .mastercard {
        background: url(/assets/images/sprite.png) 0 -1px no-repeat;
      }
      .bni {
        background: url(/assets/images/sprite.png) -50px -389px no-repeat;
      }
      .jcb {
        background: url(/assets/images/sprite.png) 5px -389px no-repeat;
      }
      .fade-enter {
        opacity: 0;
      }

      .fade-enter.fade-enter-active {
        opacity: 1;
        transition: opacity 500ms ease-in;
      }

      .fade-leave {
        opacity: 1;
      }

      .fade-leave.fade-leave-active {
        opacity: 0.01;
        transition: opacity 300ms ease-in;
      }
    </style>
  </head>
  <body>
    <script type="text/javascript" src="/assets/javascripts/application.js"></script>
  </body>
</html>
